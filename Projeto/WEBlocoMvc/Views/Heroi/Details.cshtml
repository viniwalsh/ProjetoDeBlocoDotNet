@model WEBloco.Domain.Model.Entities.Heroi

@{
    ViewData["Title"] = "Details";
}

<h1>Heroi: Ábum de Fotos</h1>

<table class="table">
    <tbody>
        <tr>
            @foreach (var item in Model.Imagens)
            {

                <td>
                    <img src="@item.FotoUri" style="width: 200px" />
                </td>

            }
        </tr>
    </tbody>
</table>


<div>
    <a asp-action="Index">Back to List</a>
</div>
